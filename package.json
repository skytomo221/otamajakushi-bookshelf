{
  "name": "otamashelf",
  "version": "0.1.0",
  "description": "OTM-JSONファイルで書かれた辞書が開けるアプリ",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint ./src --fix",
    "start": "electron ./dist/main.js",
    "build": "tsc && webpack",
    "build:main": "tsc",
    "build:render": "webpack",
    "package:mac": "webpack && electron-builder --mac --x64",
    "package:windows": "webpack && electron-builder --win --x64",
    "package:linux": "webpack && electron-builder --linux --x64"
  },
  "build": {
    "productName": "Otamajakushi Bookshelf",
    "appId": "todo.example.com",
    "files": [
      "./dist/**/*.js",
      "./dist/index.html",
      "./main.js",
      "./icon/**/*"
    ],
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "author": "skytomo",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@mdi/font": "^5.9.55",
    "@mdi/js": "^5.9.55",
    "@mdi/react": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "otamajakushi": "^1.2.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-measure": "^2.5.2",
    "react-redux": "^7.2.4",
    "react-window": "^1.8.6",
    "redux": "^4.1.0",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2"
  },
  "devDependencies": {
    "@types/material-ui": "^0.21.8",
    "@types/react": "^17.0.10",
    "@types/react-dom": "^17.0.7",
    "@types/react-measure": "^2.0.6",
    "@types/react-redux": "^7.1.16",
    "@types/react-window": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "electron": "^13.6.6",
    "electron-builder": "^22.10.5",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  }
}
